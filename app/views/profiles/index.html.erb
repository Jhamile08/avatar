
<p style="color: green"><%= notice %></p>

<% content_for :title, "Profiles" %>

<h1><%= t('profiles.title')%></h1>

<div id="profiles">
  <% @profiles.each do |profile| %>
    <%= render profile %>
    <p>
      <%= link_to t('profiles.show'), profile %>
    </p>
  <% end %>
</div>

<%= link_to t('profiles.new'), new_profile_path %>
